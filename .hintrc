{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "disown-opener": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 124",
    "not firefox <= 125"
  ]
}